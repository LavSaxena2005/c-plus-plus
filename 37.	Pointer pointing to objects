
#include<iostream>
using namespace std;
class point
{
    int x,y;
    public:
    point(int x, int y)
    {
        this->x=x;
        this->y=y;
       
    }
    void putdata()
    {
        cout<<x<<"  "<<y<<endl;


    }
};
int main()
{
    // (new point(4,5))->putdata();


    auto *c=new point(5,6);
    c->putdata();






}
